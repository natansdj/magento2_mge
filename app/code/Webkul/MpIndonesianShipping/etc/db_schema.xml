<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="wk_mpindonesianshipping_city" resource="default" engine="innodb">
        <column xsi:type="int" name="entity_id" padding="10" unsigned="true" nullable="false" identity="true"/>
        <column xsi:type="int" name="city_id" padding="10" unsigned="true" nullable="false" identity="false" default="0"/>
        <column xsi:type="varchar" name="city_name" nullable="false" length="255" default=""/>
        <column xsi:type="varchar" name="postal_code" nullable="false" length="30" default=""/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="entity_id"/>
        </constraint>
    </table>
    <table name="wk_mpindonesianshipping_country" resource="default" engine="innodb">
        <column xsi:type="int" name="entity_id" padding="10" unsigned="true" nullable="false" identity="true"/>
        <column xsi:type="int" name="country_id" padding="10" unsigned="true" nullable="false" identity="false" default="0"/>
        <column xsi:type="varchar" name="country_name" nullable="false" length="255" default=""/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="entity_id"/>
        </constraint>
    </table>
</schema>
